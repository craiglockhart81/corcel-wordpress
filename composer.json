{
    "name": "hartdevelopment/corcel-wordpress",
    "description": "Provides compatibility for wordpress functions in the latest Laravel release.",
    "keywords": ["laravel", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Developer Hartdevelopment",
            "email": "developer@hartdevelopment.co.uk"
        },
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/corcel-wordpress.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
